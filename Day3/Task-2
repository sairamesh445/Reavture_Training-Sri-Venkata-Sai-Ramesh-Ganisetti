//To check weather prime number or not
let n;
function prime(n){
    let flag=1;
    for(let i=2;i<n;i++){
        if(n%i==0){
            flag=0;
        }
    }
    if(flag==0){
        console.log("Not a Prime Number");
    }
    else if(flag==1){
        console.log("Prime Number");
    }
}
prime(13);
